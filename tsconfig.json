{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@auth/*": [
        "src/modules/auth/*"
      ],
      "@token/*": [
        "src/modules/token/*"
      ],
      "@user/*": [
        "src/modules/user/*"
      ],
      "@product/*": [
        "src/modules/product/*"
      ],
      "@review/*": [
        "src/modules/review/*"
      ],
      "@category/*": [
        "src/modules/category/*"
      ],
      "@search/*": [
        "src/modules/search/*"
      ],
      "@email/*": [
        "src/modules/email/*"
      ],
      "@offering/*": [
        "src/modules/offering/*"
      ],
      "@renting/*": [
        "src/modules/renting/*"
      ],
      "@cart/*": [
        "src/modules/cart/*"
      ],
      "@item/*": [
        "src/modules/item/*"
      ],
      "@chat/*": [
        "src/modules/chat/*"
      ],
      "@order/*": [
        "src/modules/order/*"
      ],
      "@stripe/*": [
        "src/modules/stripe/*"
      ],
      "@address/*": [
        "src/modules/address/*"
      ],
      "@subscription/*": [
        "src/modules/subscription/*"
      ],
      "@wishlist/*": [
        "src/modules/wishlist/*"
      ],
      "@storages/*": [
        "src/modules/storages/*"
      ],
      "@shipping/*": [
        "src/modules/shipping/*"
      ],
      "@inventory/*": [
        "src/modules/inventory/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}
